<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.hike_details.HikeDetailsFragment">

    <TextView
        android:id="@+id/tvHikeDetailsName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/tvHikeDetailsLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Location"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="121dp" />

    <TextView
        android:id="@+id/tvHikeDetailsDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/tvHikeDetailsLength"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Length"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="73dp" />

    <TextView
        android:id="@+id/tvHikeDetailsDifficulty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Difficulty"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="116dp" />

    <TextView
        android:id="@+id/tvHikeDetailsParkingAvailable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Parking Available"
        tools:layout_editor_absoluteX="272dp"
        tools:layout_editor_absoluteY="173dp" />

    <TextView
        android:id="@+id/tvHikeDetailsDescription"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Description"
        tools:layout_editor_absoluteX="104dp"
        tools:layout_editor_absoluteY="254dp" />

</androidx.constraintlayout.widget.ConstraintLayout>